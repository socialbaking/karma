  },
  "homepage": "https://github.com/socialbaking/vouch#readme",
  "author": "Fabian Cook <hello@fabiancook.dev>",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.2.1",
    "@fastify/request-context": "^4.2.0",
